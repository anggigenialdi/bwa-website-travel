<div class="tab-pane fade show active" id="show-feature" role="tabpanel" aria-labelledby="home-tab">

  <div class="card shadow mb-4 mt-2">
    <div class="card-body">
      <div class="row">
        <div class="col-md-5">
          <form action="/admin/bank" method="POST" enctype="multipart/form-data">
              <div class="form-group">
                <label for="exampleInputEmail1">Name</label>
                <input type="text" class="form-control" id="name" aria-describedby="name" placeholder="Enter Name"
                  name="name" required>
              </div>
              <div class="form-group">
                <label for="exampleInputEmail1">QTY</label>
                <input type="number" class="form-control" id="qty" aria-describedby="qty" placeholder="Enter qty"
                  name="qty" required>
              </div>
              <div class="form-group">
                <label for="exampleInputEmail1">Image</label>
                <input type="file" class="form-control" id="image" aria-describedby="image" name="image" required>
              </div>
              <button type="submit" class="btn btn-primary">save</button>
          </form>
        </div>
        <div class="col-md-7">
          <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
              <thead>
                <tr>
                  <th>No</th>
                  <th>Name</th>
                  <th>Qty</th>
                  <th>Image URL</th>
                  <th>Action</th>
                </tr>
              </thead>
              <tbody>
              </tbody>
            </table>
          </div>
        </div>

      </div>
    </div>
  </div>
</div>